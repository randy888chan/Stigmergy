{
  "name": "@randy888chan/stigmergy",
  "version": "2.2.0",
  "description": "Stigmergy: The Autonomous AI Development System with Universal MCP Integration for seamless IDE coordination.",
  "type": "module",
  "main": "engine/server.js",
  "bin": {
    "stigmergy": "cli/index.js"
  },
  "files": [
    "cli/",
    "dashboard/public/",
    "docs/",
    "engine/",
    "evaluation/",
    "scripts/",
    "services/",
    "src/",
    "tests/",
    "tools/",
    "utils/",
    ".stigmergy-core/",
    "README.md",
    "ecosystem.config.js",
    "stigmergy.config.js"
  ],
  "scripts": {
    "start": "node engine/server.js",
    "build": "node cli/index.js build",
    "build:dashboard": "webpack --mode production",
    "dev:dashboard": "webpack serve --mode development",
    "health-check": "node scripts/health-check.js",
    "validate": "node cli/index.js validate",
    "init": "node cli/index.js init",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration --verbose",
    "test:unit": "jest tests/unit --verbose",
    "lint": "npx eslint . --ext .js --ignore-path .gitignore",
    "lint:fix": "npx eslint . --ext .js --ignore-path .gitignore --fix",
    "coverage": "npx c8 npm test",
    "test:benchmark": "node evaluation/runners/benchmark_runner.js",
    "postinstall": "npm run build:dashboard",
    "prod:start": "pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop stigmergy-service",
    "prod:list": "pm2 list",
    "prod:logs": "pm2 logs stigmergy-service"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.2",
    "@ai-sdk/openai": "^2.0.23",
    "@babel/parser": "^7.24.5",
    "@babel/traverse": "^7.24.5",
    "@langchain/langgraph": "^0.0.11",
    "@mendable/firecrawl-js": "^0.0.28",
    "@octokit/rest": "^20.0.2",
    "@supabase/supabase-js": "^2.38.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ai": "^5.0.48",
    "axios": "^1.12.2",
    "c8": "^8.0.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.0.0",
    "compromise": "^14.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "express": "^4.21.2",
    "file-type": "^18.7.0",
    "glob": "^10.3.10",
    "inquirer": "^12.9.4",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "mammoth": "^1.6.0",
    "multer": "^2.0.2",
    "neo4j-driver": "^5.19.0",
    "node-cache": "^5.1.2",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "pdf-parse-debugging-disabled": "^1.1.1",
    "pdf2pic": "^3.0.1",
    "pm2": "^6.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-syntax-highlighter": "^5.8.0",
    "recharts": "^3.2.0",
    "similarity": "^1.2.1",
    "tree-sitter": "^0.20.4",
    "tree-sitter-javascript": "^0.20.1",
    "typescript": "^5.3.3",
    "ws": "^8.18.3",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "buffer": "^6.0.3",
    "css-loader": "^7.1.1",
    "fs-extra": "^11.3.2",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
