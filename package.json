    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "memfs": "^4.39.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "highlight.js": "11.9.0"
  }
}
